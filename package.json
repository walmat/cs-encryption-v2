    "coveralls": "^3.1.0",
    "dot-json": "^1.2.1",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.3",
    "tslint": "^6.1.3",
    "ts-node": "9.0.0",
    "ts-node-dev": "1.0.0",
    "typedoc": "0.19.2",
    "typedoc-plugin-external-module-name": "4.0.3",
